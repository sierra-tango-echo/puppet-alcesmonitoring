# Generic NRPE commands
command[check_users]=/usr/lib64/nagios/plugins/check_users -w 50 -c 100
command[check_load]=/usr/lib64/nagios/plugins/check_load -w 170,150,130 -c 200,170,150
command[check_disk]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /dev/root
command[check_disk_var]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /var
command[check_disk_tmp]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /tmp
#command[check_disk_users]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /users
#command[check_disk_alces]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /opt/alces
#command[check_disk_gridware]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /opt/gridware
command[check_zombie_procs]=/usr/lib64/nagios/plugins/check_procs -w 5 -c 10 -s Z
command[check_total_procs]=/usr/lib64/nagios/plugins/check_procs -w 2000 -c 3000

# Site specfic commands
command[check_smart_attributes_sda]=/usr/bin/sudo /usr/lib64/nagios/plugins/check_smart_attributes --ata --text --drive=/dev/sda
command[check_smart_attributes_sdb]=/usr/bin/sudo /usr/lib64/nagios/plugins/check_smart_attributes --ata --text --drive=/dev/sdb
