# Generic NRPE commands
command[check_users]=/usr/lib64/nagios/plugins/check_users -w 50 -c 100
command[check_load]=/usr/lib64/nagios/plugins/check_load -w 170,150,130 -c 200,170,150
command[check_disk]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /dev/root
command[check_disk_var]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /var
command[check_disk_tmp]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /tmp
command[check_disk_users]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /users
command[check_disk_alces]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /opt/alces
command[check_disk_gridware]=/usr/lib64/nagios/plugins/check_disk -w 20% -c 10% -p /opt/gridware
command[check_zombie_procs]=/usr/lib64/nagios/plugins/check_procs -w 5 -c 10 -s Z
command[check_total_procs]=/usr/lib64/nagios/plugins/check_procs -w 2000 -c 3000

# Hardware specific commands
command[check_PERC_H7X0]=sudo /var/lib/alces/share/nagios/plugins/check_PERC_H7X0 0 0
command[check_PERC_H8X0]=sudo /var/lib/alces/share/nagios/plugins/check_PERC_H7X0 0 1
command[check_H7X0_PF]=sudo /var/lib/alces/share/nagios/plugins/check_H7X0_PF
command[check_PSUs]=sudo /var/lib/alces/share/nagios/plugins/check_PSUs

# Site specfic commands
command[check_headary]=sudo /var/lib/alces/share/nagios/plugins/check_dellMDarray headarray1
command[check_ossary1]=sudo /var/lib/alces/share/nagios/plugins/check_dellMDarray ossarray1
command[check_ossary2]=sudo /var/lib/alces/share/nagios/plugins/check_dellMDarray ossarray2
command[check_ossary3]=sudo /var/lib/alces/share/nagios/plugins/check_dellMDarray ossarray3
command[check_stgary1]=sudo /var/lib/alces/share/nagios/plugins/check_dellMDarray stgary1
command[check_stgary2]=sudo /var/lib/alces/share/nagios/plugins/check_dellMDarray stgary2

command[check_GE]=/var/lib/alces/share/nagios/plugins/check_GE
command[check_dirvish]=sudo /var/lib/alces/share/nagios/plugins/check_dirvish
